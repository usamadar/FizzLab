{
  "expo": {
    "name": "FizzLab",
    "slug": "fizzlab",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fizzlab-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.FizzLab"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fizzlab-adaptive.png",
        "backgroundImage": "./assets/images/fizzlab-background.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.FizzLab"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/fizzlab-favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fizzlab-icon-full.jpeg",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "350cf717-3ebd-4385-8be2-3106697a1207"
      }
    },
    "owner": "usamadar"
  }
}
